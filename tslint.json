{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "no-console": [true, "log", "warn"],
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "esSpecCompliant": true
            }
        ],
        "object-literal-sort-keys": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}