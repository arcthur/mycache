{
  "name": "mycache",
  "version": "0.5.0",
  "description": "A Cache library",
  "keywords": [
    "cache",
    "indexedDB",
    "localstorage",
    "javascript"
  ],
  "homepage": "https://github.com/arcthur/mycache#readme",
  "bugs": {
    "url": "https://github.com/arcthur/mycache/issues"
  },
  "license": "MIT",
  "authors": [
    "arcthur <arthurtemptation@gmail.com> (https://github.com/arcthur)"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/cjs",
  "module": "dist/esm/mycache.prod.min.js",
  "browser": "dist/umd/mycache.prod.min.js",
  "types": "src/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arcthur/mycache.git"
  },
  "scripts": {
    "build": "cross-env rimraf lib build dist && tsc && rollup -c && node ./scripts/bundles",
    "dev": "tsc --watch src",
    "lint": "tslint src/*",
    "test": "npm run build && nyc ava",
    "report": "nyc report --reporter=html"
  },
  "dependencies": {
    "localforage": "^1.7.2"
  },
  "devDependencies": {
    "@types/node": "^9.6.0",
    "ava": "^0.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "nyc": "^11.6.0",
    "puppeteer": "^1.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "uglify-es": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "sources": [
      "**/*.js",
      "!dist/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "es2015"
      ]
    }
  }
}
